    background-color: rgb(31,31,31);
}

ZenoWelcomePage {
    background-color: rgb(31,31,31);
}

QWidget[cssClass="docktoolbar"] {
    background-color: rgb(31,31,31);
}

ZTimeline {
    background-color: rgb(31,31,31);
    border:1px solid rgb(61,61,61);
}

ZenoPropPanel {
    background-color: rgb(31,31,31);
}

ZContentWidget {
    background-color: rgb(31,31,31);
}

LayerPathWidget {
    background-color: rgba(31,31,31, 0);
}

